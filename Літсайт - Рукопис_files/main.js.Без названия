var fb_timeout=null,fb_opts={overlayShow:!0,hideOnOverlayClick:!0,showCloseButton:!0,centerOnScroll:!0,enableEscapeButton:!0,autoScale:!0},easy_fancybox_handler=function(){var o='a[href*=".jpg"]:not(.nofancybox,.pin-it-button), area[href*=".jpg"]:not(.nofancybox), a[href*=".jpeg"]:not(.nofancybox,.pin-it-button), area[href*=".jpeg"]:not(.nofancybox), a[href*=".gif"]:not(.nofancybox,.pin-it-button), area[href*=".gif"]:not(.nofancybox), a[href*=".png"]:not(.nofancybox,.pin-it-button), area[href*=".png"]:not(.nofancybox)';jQuery(o).addClass("fancybox image").attr("rel","gallery"),jQuery("a.fancybox, area.fancybox, li.fancybox a:not(li.nofancybox a)").fancybox(jQuery.extend({},fb_opts,{easingIn:"easeOutBack",easingOut:"easeInBack",opacity:!1,hideOnContentClick:!1,titleShow:!0,titlePosition:"over",titleFromAlt:!0,showNavArrows:!0,enableKeyboardNav:!0,cyclic:!1})),jQuery('a[href*="youtube.com/watch"]:not(.nofancybox), area[href*="youtube.com/watch"]:not(.nofancybox)').addClass("fancybox-youtube"),jQuery('a[href*="youtu.be/"]:not(.nofancybox), area[href*="youtu.be/"]:not(.nofancybox)').addClass("fancybox-youtube"),jQuery("a.fancybox-youtube, area.fancybox-youtube, li.fancybox-youtube a:not(li.nofancybox a)").fancybox(jQuery.extend({},fb_opts,{type:"iframe",width:640,height:360,padding:0,titleShow:!1,titlePosition:"float",titleFromAlt:!0,onStart:function(o,e,a){a.href=o[e].href.replace(new RegExp("youtu.be","i"),"www.youtube.com/embed").replace(new RegExp("watch\\?(.*)v=([a-z0-9_-]+)(&|&|\\?)?(.*)","i"),"embed/$2?$1$4");var n=a.href.indexOf("?"),t=n>-1?a.href.substring(n):"";a.allowfullscreen=t.indexOf("fs=0")>-1?!1:!0}})),jQuery("#fancybox-auto").trigger("click")};
jQuery('.slide_show').hide();
jQuery('.slide_title').on('click', function(){
	jQuery(this).next().slideToggle();
});